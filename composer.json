{
  "name": "bytic/report-generator",
  "description": "report generator library",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "facebook",
    "pixel"
  ],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/bytic/report-generator",
  "require": {
    "php": "^8.0",
    "bytic/collections": "^0.9 || ^1.0 || ^2.0",
    "bytic/package-base": "^1.0",
    "phpoffice/phpspreadsheet": "^5.3",
    "symfony/http-foundation": "^5.0 || ^6.0 || ^7.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\ReportGenerator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\ReportGenerator\\Tests\\": "tests/src",
      "ByTIC\\ReportGenerator\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
